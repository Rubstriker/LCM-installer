{
  "private": true,
  "version": "1.0.0",
  "name": "lcm",
  "scripts": {
    "build": "tsc",
    "package": "node .lcm/scripts/package.js",
    "start": "ts-node src/index.ts",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix"
  },
  "dependencies": {
    "chalk": "4",
    "cheerio": "^1.0.0-rc.12",
    "glob": "^10.3.3",
    "moment": "^2.29.4",
    "no-try": "^3.1.0",
    "vdf-parser": "^1.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "strip-ansi": "^6.0.1"
  },
  "pkg": {
    "assets": [
      "lcm-data/**/*"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "out"
  },
  "bin": {
    "lcm": "./dist/index.js"
  }
}
